void notifyClients(char* msg) {
  ws.textAll(msg);
}

void sendUartMsg(char* msg) {
  char* processedMsg = filterMsg(msg);

  Serial.write(processedMsg);
}

char* filterMsg(char* msg) {
  char* result = msg;
  int len = strlen(msg);
  bool hasTernaryOperator = false;

  for (int i = 0; i < len; i++) {
    char c = msg[i];
    bool isInvalidChar = checkChar(c);

    if (c == '\r') hasTernaryOperator = true;

    if (!isInvalidChar) continue;

    for(int j = i; j < len; j++) result[j] = '\0';

    break;
  }

  if (!hasTernaryOperator) result[strlen(result)] ='\r';

  return result;
}

bool checkChar(char c) {
  int allowedCharsCount = sizeof(allowedChars) / sizeof(allowedChars[0]);

  if (isalpha(c) || isdigit(c)) return false;

  for (int i = 0; i < allowedCharsCount; i++) {
    if (c == allowedChars[i]) return false;
  }

  return true;
}

